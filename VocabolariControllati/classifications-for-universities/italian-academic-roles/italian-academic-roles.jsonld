    {
      "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/7",
      "@type": [
        "herapit:ItalianAcademicRole",
        "skos:Concept"
      ],
      "clvapit:hasRankOrder": "1",
      "dct:identifier": "7",
      "skos:definition": {
        "@language": "it",
        "@value": "Il professore straordinario è un docente che si trova nel triennio precedente alla nomina di professore ordinario. Nella precedente riforma universitaria, prima di prendere servizio definitivamente nel ruolo di professori di I fascia era previsto un triennio a seguito del quale veniva effettuata una valutazione scientifica e didattica. Ciò non trova più riscontro nella normativa vigente."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
      },
      "skos:notation": "7",
      "skos:prefLabel": {
        "@language": "it",
        "@value": "Professore straordinario"
      },
      "xkos:depth": 1
    },
    {
      "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/6",
      "@type": [
        "skos:Concept",
        "herapit:ItalianAcademicRole"
      ],
      "clvapit:hasRankOrder": "1",
      "dct:identifier": "6",
      "skos:definition": {
        "@language": "it",
        "@value": "Il professore ordinario (detto anche Professore di I fascia) è un docente titolare di cattedra e rappresenta la più alta qualifica nella carriera accademica."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
      },
      "skos:notation": "6",
      "skos:prefLabel": {
        "@language": "it",
        "@value": "Professore ordinario"
      },
      "xkos:depth": 1
    },
    {
      "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/5",
      "@type": [
        "herapit:ItalianAcademicRole",
        "skos:Concept"
      ],
      "clvapit:hasRankOrder": "1",
      "dct:identifier": "5",
      "owl:versionInfo": [
        {
          "@language": "it",
          "@value": "Ruolo a esaurimento"
        },
        {
          "@language": "en",
          "@value": "Not available for new recruitments"
        }
      ],
      "skos:definition": {
        "@language": "it",
        "@value": "Il professore incaricato era una figura docente cui veniva affidato un insegnamento presso un corso di studi universitario. Due provvedimenti di legge garantirono la continuità dell'insegnamento a chi avesse insegnato una data materia per un triennio consecutivo."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
      },
      "skos:notation": "5",
      "skos:prefLabel": {
        "@language": "it",
        "@value": "Professore incaricato"
      },
      "xkos:depth": 1
    },
    {
      "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/4",
      "@type": [
        "skos:Concept",
        "herapit:ItalianAcademicRole"
      ],
      "clvapit:hasRankOrder": "1",
      "dct:identifier": "4",
      "skos:definition": {
        "@language": "it",
        "@value": "Il professore associato non confermato è un docente che si trova nel triennio precedente alla nomina di professore associato. Nella precedente riforma universitaria, prima di prendere servizio definitivamente nel ruolo di professori di II fascia era previsto un triennio a seguito del quale veniva effettuata una valutazione scientifica e didattica. Ciò non trova più riscontro nella normativa vigente."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
      },
      "skos:notation": "4",
      "skos:prefLabel": {
        "@language": "it",
        "@value": "Professore associato non confermato"
      },
      "xkos:depth": 1
    },
    {
      "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/3",
      "@type": [
        "skos:Concept",
        "herapit:ItalianAcademicRole"
      ],
      "clvapit:hasRankOrder": "1",
      "dct:identifier": "3",
      "skos:definition": {
        "@language": "it",
        "@value": "Il professore associato (detto anche Professore di II fascia) è un docente titolare di cattedra. Differisce dal Professore di I fascia in quanto si trova in una posizione più bassa nella carriera accademica."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
      },
      "skos:notation": "3",
      "skos:prefLabel": {
        "@language": "it",
        "@value": "Professore associato"
      },
      "xkos:depth": 1
    },
    {
      "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/2",
      "@type": [
        "skos:Concept",
        "herapit:ItalianAcademicRole"
      ],
      "clvapit:hasRankOrder": "1",
      "dct:identifier": "2",
      "owl:versionInfo": [
        {
          "@language": "en",
          "@value": "Not available for new recruitments"
        },
        {
          "@language": "it",
          "@value": "Ruolo a esaurimento"
        }
      ],
      "skos:definition": {
        "@language": "it",
        "@value": "Gli assistenti di ruolo coadiuvano il professore della loro disciplina nella ricerca scientifica e nell'attività didattica, con particolare riguardo alle esercitazioni. Il ruolo è ad esaurimento. Il solo titolo di studio necessario era la laurea. Il concorso era su base locale e per la singola disciplina, e dopo due anni era previsto il giudizio di conferma da parte della Facoltà."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
      },
      "skos:notation": "2",
      "skos:prefLabel": {
        "@language": "it",
        "@value": "Assistente"
      },
      "xkos:depth": 1
    },
    {
      "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles/1",
      "@type": [
        "skos:Concept",
        "herapit:ItalianAcademicRole"
      ],
      "clvapit:hasRankOrder": "1",
      "dct:identifier": "1",
      "skos:definition": {
        "@language": "it",
        "@value": "L’assegnista di ricerca è una figura titolare di borsa di studio (chiamata anche assegno di ricerca) per svolgere attività di ricerca. Viene individuato attraverso una procedura di selezione pubblica sulla base di uno specifico bando e deve necessariamente essere in possesso di curriculum scientifico-professionale idoneo allo svolgimento di tale attività."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
      },
      "skos:notation": "1",
      "skos:prefLabel": {
        "@language": "it",
        "@value": "Assegnista di ricerca"
      },
      "xkos:depth": 1
    },
