    <!-- https://w3id.org/italia/onto/Project/ProcurementProject -->

    <owl:Class rdf:about="https://w3id.org/italia/onto/Project/ProcurementProject">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/Project/PublicInvestmentProject"/>
        <owl:disjointWith rdf:resource="https://w3id.org/italia/onto/Project/PublicResearchProject"/>
        <rdfs:comment xml:lang="it">La classe che comprende i progetti atti a soddisfare un fabbisogno individuato dall&apos;amministrazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">The class that includes projects aiming at meet the needs detected by a public administration.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/Project"/>
        <rdfs:label xml:lang="en">Procurement Project</rdfs:label>
        <rdfs:label xml:lang="it">Progetto di approvigionamento</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- https://w3id.org/italia/onto/Project/PublicInvestmentProject -->

    <owl:Class rdf:about="https://w3id.org/italia/onto/Project/PublicInvestmentProject">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Object"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/Project/projectFunder"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/CLV/hasSpatialCoverage"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/CLV/Feature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/Project/hasParticipantingAgent"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/RO/hasRiT"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/RO/TimeIndexedRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
