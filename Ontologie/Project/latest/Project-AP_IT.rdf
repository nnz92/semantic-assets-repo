    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/Project/hasParticipantingAgent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/Project/hasParticipantingAgent">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/Project/involvesEntity"/>
