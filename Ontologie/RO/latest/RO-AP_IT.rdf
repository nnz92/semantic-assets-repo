    <!-- https://w3id.org/italia/onto/RO/forEntity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/forEntity">
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/RO/hasRiT"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/RO/TimeIndexedRole"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Entity"/>
        <rdfs:comment xml:lang="en">Property that links a time indexed role to an entity (for which onother plays a role).</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà che collega la situazione (per la gestione del ruolo) a un&apos;entità per cui un&apos;altra ha un ruolo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO"/>
        <rdfs:label xml:lang="en">for entity</rdfs:label>
        <rdfs:label xml:lang="it">per entità</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">versione non definitiva</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/RO/hasRiT -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/hasRiT">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/l0/Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/RO/TimeIndexedRole"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;for entity&quot;</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inverse di &quot;per entità&quot;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO"/>
        <rdfs:label xml:lang="it">ha ruolo nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">has role in time</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">versione non definitiva</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/RO/holdsRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/holdsRole">
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/RO/isRoleOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/l0/Entity"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/RO/Role"/>
        <rdfs:comment xml:lang="it">La relazione tra un&apos;entità e un ruolo.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation existing between an entity and a role.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO"/>
        <rdfs:label xml:lang="en">holds role</rdfs:label>
        <rdfs:label xml:lang="it">ricopre ruolo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/RO/holdsRoleInTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/holdsRoleInTime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/RO/hasRiT"/>
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/RO/isRoleInTimeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/RO/TimeIndexedRole"/>
        <rdfs:comment xml:lang="it">Relazione tra un&apos;entità generica ed un ruolo nel tempo.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation between an entity and a role in time.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO/"/>
        <rdfs:label xml:lang="en">holds role in time</rdfs:label>
        <rdfs:label xml:lang="it">riveste ruolo nel tempo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/RO/isRoleIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/isRoleIn">
        <owl:inverseOf rdf:resource="https://w3id.org/italia/onto/RO/withRole"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/RO/Role"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/RO/TimeIndexedRole"/>
        <rdfs:comment xml:lang="en">Inverse property of &quot;with role&quot;</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di &quot;con ruolo&quot;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO/"/>
        <rdfs:label xml:lang="en">is role in</rdfs:label>
        <rdfs:label xml:lang="it">è ruolo in</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/RO/isRoleInTimeOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/isRoleInTimeOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/RO/TimeIndexedRole"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="en">Inverse property of holds role in time</rdfs:comment>
        <rdfs:comment xml:lang="it">Proprietà inversa di ha ruolo nel tempo</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO"/>
        <rdfs:label xml:lang="en">is role in time of</rdfs:label>
        <rdfs:label xml:lang="it">è ruolo nel tempo di</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/RO/isRoleOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/isRoleOf">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/RO/Role"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Entity"/>
        <rdfs:comment xml:lang="it">La relazione tra un ruolo ed un&apos;entità. E&apos; la proprietà inversa di holdsRole.</rdfs:comment>
        <rdfs:comment xml:lang="en">The relation existing between a role and an entity. This is the inverse property of holdsRole.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO"/>
        <rdfs:label xml:lang="en">is role of</rdfs:label>
        <rdfs:label xml:lang="it">è ruolo di</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/RO/withRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/RO/withRole">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/RO/TimeIndexedRole"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/RO/Role"/>
        <rdfs:comment xml:lang="en">Association between a TimeIndexedRole and a Role.</rdfs:comment>
        <rdfs:comment xml:lang="it">Associazione tra un TimeIndexedRole ed un Role.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/italia/onto/RO/"/>
        <rdfs:label xml:lang="it">con ruolo</rdfs:label>
        <rdfs:label xml:lang="en">with role</rdfs:label>
    </owl:ObjectProperty>
    


