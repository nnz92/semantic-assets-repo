      "skos:definition": {
        "@language": "it",
        "@value": "Il ricercatore a tempo indeterminato è una figura che contribuisce allo sviluppo della ricerca scientifica universitaria e assolve compiti didattici integrativi. Si tratta di una figura ad esaurimento."
      },
      "skos:inScheme": {
        "@id": "https://w3id.org/italia/controlled-vocabulary/classifications-for-universities/italian-academic-roles"
